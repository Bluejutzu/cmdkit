generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String   @id
  uuid          String
  createdAt     DateTime @default(now())
  discordName   String?
  minecraftName String?
  role          Role     @default(USER)
  token         String?
  code          String?
}

enum Role {
  USER
  OPERATOR
  ADMIN
}
