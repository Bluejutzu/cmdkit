{
  "compilerOptions": {
    "target": "es5",
    "module": "ES2022",
    "moduleResolution": "Node",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "commandkit": ["./node_modules/commandkit"],
      "discord.js": ["./node_modules/discord.js"],
      "db": ["./src/db/index.ts"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
